<div class="container">
    <h1>Estudiantes por Empresa</h1> <!-- Título de la página -->

    <div class="row mb-2">
        <link rel="stylesheet" type="text/css" href="/css/projectsTable.css">
        <table class="table table-hover text-center mb-5 ">
            <thead>
                <tr>
                    <th>Estudiante</th>
                    <th>Carnet</th>
                    <th>Telefono</th>
                    <th>Correo</th>
                    <th>Empresa</th>
                    <th>Profesor</th>
                    <th>Semestre</th>
                </tr>
            </thead>
            <tbody>
                {{#each data}}
                <tr>
                    <td>{{this.estudiante_info.nombre}}</td>
                    <td>{{this.estudiante_info.carnet}}</td>
                    <td>{{this.estudiante_info.telefono}}</td>
                    <td>{{this.estudiante_info.correo}}</td>
                    <td>{{this.nombreEmpresa}}</td>
                    <td>{{this.profesor_info.nombre}}</td>
                    <td>{{this.semestre}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>



    </div>
</div>